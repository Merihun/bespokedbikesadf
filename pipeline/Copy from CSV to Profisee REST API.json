{
	"name": "Copy from CSV to Profisee REST API",
	"properties": {
		"activities": [
			{
				"name": "Copy from CSV to Profisee REST API",
				"description": "Copy records from ADLS Gen2 in CSV format to Profisee via the Profisee REST API.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"modifiedDatetimeStart": "2023-05-14T00:00:00.000Z",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "RestSink",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": 10,
						"requestMethod": "PATCH",
						"writeBatchSize": 10000,
						"httpCompressionType": "none"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SourceFile",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ProfiseeRESTAPI_Records_Upsert",
						"type": "DatasetReference",
						"parameters": {
							"entityId": {
								"value": "@replace(pipeline().parameters.FileFolder, 'profisee-input/', '')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "Mountaventure.csv"
			},
			"FileFolder": {
				"type": "string",
				"defaultValue": "profisee-input/product"
			}
		},
		"folder": {
			"name": "CSV Format"
		},
		"annotations": [],
		"lastPublishTime": "2020-09-25T15:29:08Z"
	}
}