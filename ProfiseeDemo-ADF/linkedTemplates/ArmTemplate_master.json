{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ProfiseeDemo-ADF"
		},
		"ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage'"
		},
		"ls_azure_datalake_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_azure_datalake_storage'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KeyVaultProfisee.vault.azure.net/"
		},
		"AzureKeyVault2_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://NewKeyVaultProfisee.vault.azure.net/"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sbprofisee.onebridge.xyz/profisee/rest/v1"
		},
		"RestService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "X-API-Key"
		},
		"ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://profiseedemosa.dfs.core.windows.net/"
		},
		"ls_AzureKV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://fhi360demoKV.vault.azure.net/"
		},
		"ls_ProfiseeRESTAPI_upsert_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sbprofisee.onebridge.xyz/profisee/rest/v1"
		},
		"ls_ProfiseeRESTAPI_upsert_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "X-API-Key"
		},
		"ls_azure_datalake_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://profiseedemostorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ProfiseeDemo-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage_accountKey')]"
					},
					"ls_azure_datalake_storage_accountKey": {
						"value": "[parameters('ls_azure_datalake_storage_accountKey')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault2_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault2_properties_typeProperties_baseUrl')]"
					},
					"ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_AzureKV_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKV_properties_typeProperties_baseUrl')]"
					},
					"ls_azure_datalake_storage_properties_typeProperties_url": {
						"value": "[parameters('ls_azure_datalake_storage_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_userName": {
						"value": "[parameters('RestService1_properties_typeProperties_userName')]"
					},
					"ls_ProfiseeRESTAPI_upsert_properties_typeProperties_url": {
						"value": "[parameters('ls_ProfiseeRESTAPI_upsert_properties_typeProperties_url')]"
					},
					"ls_ProfiseeRESTAPI_upsert_properties_typeProperties_userName": {
						"value": "[parameters('ls_ProfiseeRESTAPI_upsert_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}